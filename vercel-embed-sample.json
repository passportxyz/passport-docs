{
  "name": "passport-embed-demo",
  "version": 2,
  "builds": [
    {
      "src": "sample-apps/embed-demo/package.json",
      "use": "@vercel/next",
      "config": {
        "rootDirectory": "sample-apps/embed-demo"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sample-apps/embed-demo/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_PASSPORT_API_KEY": "@passport-embed-api-key",
    "NEXT_PUBLIC_PASSPORT_SCORER_ID": "@passport-scorer-id", 
    "PASSPORT_API_KEY": "@passport-stamps-api-key",
    "NEXT_PUBLIC_REOWN_PROJECT_ID": "@reown-project-id"
  }
}